# Local Development Environment Variables
# Copy this file to .env and fill in your actual values

# Demo mode - set to true to use mock data (recommended)
VITE_DEMO_MODE=true

# API Base URL (only needed if not using demo mode)
VITE_API_BASE_URL=http://localhost:3001/api

# Claude AI API Key (optional - for enhanced resume parsing)
# Get your API key from: https://console.anthropic.com/account/keys
# This enables AI-powered skill extraction from resumes
VITE_CLAUDE_API_KEY=